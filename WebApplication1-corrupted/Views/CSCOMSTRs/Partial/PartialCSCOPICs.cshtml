@model IEnumerable<WebApplication1.CSCOPIC>
    @using WebApplication1.Utility;


    

<table class="table table-bordered table-hover table-responsive-lg table-sm">
    <tr class="label-primary">

        <th>
            @Html.DisplayNameFor(model => model.CSPR.PRSNAME)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CSPR.PRSCODE)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DESIG)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.REM)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.STAMP)
        </th>

        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr class="text-nowrap">
            <td>
                @Html.DisplayFor(modelItem => item.CSPR.PRSNAME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CSPR.PRSCODE)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.DESIG)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.REM)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.STAMP)
            </td>

            <td>
                @Html.ActionLink(" ", "Edit", "CSCOPICs", new { id = item.CONO, person = item.PRSCODE }, new { @class = "fa fa-edit", title = "Edit" }) |
                @Html.ActionLink(" ", "Details", "CSCOPICs", new { id = item.CONO, person = item.PRSCODE }, new { @class = "fa fa-folder", title = "Details" }) |
                @Html.ActionLink(" ", "Delete", "CSCOPICs", new { id = item.CONO, person = item.PRSCODE }, new { @class = "fa fa-trash", title = "Delete" })
            </td>
        </tr>
    }

</table>
